{
    "name": "react-default-it",
    "displayName": "React Default It",
    "description": "Generate defaultProps based on propTypes",
    "version": "0.8.1",
    "publisher": "kserjey",
    "icon": "images/icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "react",
        "defautProps",
        "propTypes"
    ],
    "activationEvents": [
        "onCommand:extension.defaultIt"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.defaultIt",
                "title": "Generate defaultProps",
                "category": "DefaultIt"
            }
        ]
    },
    "scripts": {
        "nearley:build": "nearleyc ./src/grammar.ne --out ./src/grammar.js",
        "nearley:test": "nearley-test ./src/grammar.js",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "eslint": "^5.5.0",
        "vscode": "^1.1.21"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kserjey/react-default-it.git"
    },
    "dependencies": {
        "nearley": "^2.15.1"
    }
}
